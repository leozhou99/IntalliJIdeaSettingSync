<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.iml;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.idea;.svn;CVS;__pycache__;_svn;gradlew;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern="111" type="AUTO_DETECTED" />
      <mapping ext="space.kts" type="Kotlin" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping ext="pkl" type="PLAIN_TEXT" />
      <mapping ext="wxss" type="PLAIN_TEXT" />
      <removed_mapping ext="json" approved="true" type="JSON" />
      <removed_mapping ext="yaml" approved="true" type="YAML" />
    </extensionMap>
  </component>
</application>