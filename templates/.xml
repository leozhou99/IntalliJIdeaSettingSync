<templateSet group="自定义">
  <template name="*" value="*&#10;* $Description$&#10;* @Author $USER$&#10;* @Date $Date$ $Time$ $params$ $return$&#10;*/" shortcut="ENTER" description="" toReformat="false" toShortenFQNames="true">
    <variable name="Description" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="Date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="Time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="groovyScript(&quot;def result = '';def params = \&quot;${_1}\&quot;.replaceAll('[\\\\[|\\\\]|\\\\s]', '').split(',').toList(); for(i = 0; i &lt; params.size(); i++) {if(params[i] != '')result+='* @param ' + params[i] + ((i &lt; params.size() - 1) ? '\\r\\n ' : '')}; return result == '' ? null : '\\r\\n ' + result&quot;, methodParameters())" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="groovyScript(&quot;return \&quot;${_1}\&quot; == 'void' ? null : '\\r\\n * @return ' + \&quot;${_1}\&quot;&quot;, methodReturnType())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="nef" value="if(org.apache.commons.collections4.CollectionUtils.isNotEmpty($COLLECTION$)){&#10;    for ($ELEMENT_TYPE$ $VAR$ : $COLLECTION$) {&#10;        $END$&#10;    }&#10;}" description="非空遍历集合" toReformat="true" toShortenFQNames="true">
    <variable name="ELEMENT_TYPE" expression="iterableComponentType(COLLECTION)" defaultValue="&quot;java.lang.Object&quot;" alwaysStopAt="false" />
    <variable name="COLLECTION" expression="variableOfType(&quot;java.util.Collection&quot;)" defaultValue="" alwaysStopAt="false" />
    <variable name="VAR" expression="suggestVariableName()" defaultValue="&quot;one&quot;" alwaysStopAt="false" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="nefa" value="if ($ARRAY$ != null &amp;&amp; $ARRAY$.length &gt; 0) {&#10;    for ($ELEMENT_TYPE$ $VAR$ : $ARRAY$) {&#10;        &#10;    }&#10;}" description="非空遍历数组" toReformat="false" toShortenFQNames="true">
    <variable name="ARRAY" expression="arrayVariable()" defaultValue="" alwaysStopAt="false" />
    <variable name="ELEMENT_TYPE" expression="iterableComponentType(ARRAY)" defaultValue="" alwaysStopAt="false" />
    <variable name="VAR" expression="suggestVariableName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>