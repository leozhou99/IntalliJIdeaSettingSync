<templateSet group="zdy">
  <template name="nef" value="if(org.apache.commons.collections4.CollectionUtils.isNotEmpty($COLLECTION$)){&#10;    for ($ELEMENT_TYPE$ $VAR$ : $COLLECTION$) {&#10;        $END$&#10;    }&#10;}" description="非空遍历集合" toReformat="false" toShortenFQNames="true">
